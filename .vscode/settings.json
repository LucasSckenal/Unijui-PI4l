{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 16,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.tabSize": 1,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "tabnine.experimentalAutoImports": true,
  "git.autofetch": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "cSpell.language": "en,pt-BR",
  "gitlens.views.commitDetails.files.layout": "tree",
  "editor.formatOnPaste": true,
  "cSpell.autoFormatConfigFile": true,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "RainbowBrackets.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    ["yellow", "blueviolet", "aqua", "violet", "green"],
    "Revioletd"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "liveServer.settings.donotVerifyTags": true,
  "gitlens.views.repositories.showUpstreamStatus": false,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command"
  ],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "figma.autocompleteBlocks": true,
  "gitlens.views.branches.branches.layout": "list",
  "terminal.integrated.env.windows": {},
  "git.confirmSync": false,
  "java.debug.settings.console": "internalConsole",
  "console-ninja.featureSet": "Community",
  "RainbowBrackets.depreciation-notice": false,
  "maven.terminal.useJavaHome": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    {
      "column": 80,
      "color": "#00FF0010"
    },
    {
      "column": 100,
      "color": "#BDB76B15"
    },
    {
      "column": 120,
      "color": "#FA807219"
    }
  ],
  "editor.unicodeHighlight.includeComments": true,
  "emmet.variables": {
    "lang": "en"
  },
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "tab.activeBorderTop": "#00FF00",
      "tab.unfocusedActiveBorderTop": "#00FF0088",
      "textCodeBlock.background": "#00000055"
    },
    "editor.wordHighlightStrongBorder": "#FF6347",
    "editor.wordHighlightBorder": "#FFD700",
    "editor.selectionHighlightBorder": "#A9A9A9"
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.tree.indent": 20,
  "files.eol": "\n",
  "[bat]": {
    "files.eol": "\r\n"
  },
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.tabs.hideCondition": "never",
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.debug.settings.hotCodeReplace": "auto",
  "java.sources.organizeImports.staticStarThreshold": 1,
  "cSpell.diagnosticLevel": "Hint",
  "trailing-spaces.includeEmptyLines": false,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "workbench.colorTheme": "Tokyo Night",
  "hediet.vscode-drawio.resizeImages": null
}
